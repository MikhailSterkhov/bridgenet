<?xml version="1.0" encoding="UTF-8" ?>
<interceptor>
    <methodHandlers>
        <methodHandler>
            <name>Async</name>
            <annotation>me.moonways.bridgenet.api.inject.decorator.persistance.Async</annotation>
            <path>me.moonways.bridgenet.api.inject.decorator.persistance.handler.AsyncMethodHandler</path>
        </methodHandler>
        <methodHandler>
            <name>LateExecution</name>
            <annotation>me.moonways.bridgenet.api.inject.decorator.persistance.LateExecution</annotation>
            <path>me.moonways.bridgenet.api.inject.decorator.persistance.handler.LateExecutionMethodHandler</path>
            <conflicts>
                <input>Async</input>
                <input>Singleton</input>
            </conflicts>
        </methodHandler>
        <methodHandler>
            <name>KeepTime</name>
            <annotation>me.moonways.bridgenet.api.inject.decorator.persistance.KeepTime</annotation>
            <path>me.moonways.bridgenet.api.inject.decorator.persistance.handler.KeepTimeMethodHandler</path>
            <inherits>
                <input>Async</input>
                <input>LateExecution</input>
            </inherits>
        </methodHandler>
        <methodHandler>
            <name>Singleton</name>
            <annotation>me.moonways.bridgenet.api.inject.decorator.persistance.Singleton</annotation>
            <path>me.moonways.bridgenet.api.inject.decorator.persistance.handler.SingletonMethodHandler</path>
            <inherits>
                <input>Async</input>
                <input>LateExecution</input>
            </inherits>
        </methodHandler>
        <methodHandler>
            <name>RequiredNotNull</name>
            <annotation>me.moonways.bridgenet.api.inject.decorator.persistance.RequiredNotNull</annotation>
            <path>me.moonways.bridgenet.api.inject.decorator.persistance.handler.RequiredNotNullMethodHandler</path>
            <inherits>
                <input>Async</input>
                <input>LateExecution</input>
            </inherits>
        </methodHandler>
    </methodHandlers>
</interceptor>
