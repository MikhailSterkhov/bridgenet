<?xml version="1.0" encoding="UTF-8" ?>
<bootstrap>
    <hooks>
        <hook>
            <displayName>JDBC</displayName>
            <priorityID>1</priorityID>
            <priority>RUNNER</priority>
            <executorPath>me.moonways.bridgenet.bootstrap.hook.jdbc.ConvenoJdbcHook</executorPath>
        </hook>
        <hook>
            <displayName>MessageTransferProtocol [MTP]</displayName>
            <priorityID>2</priorityID>
            <priority>RUNNER</priority>
            <executorPath>me.moonways.bridgenet.bootstrap.hook.mtp.MTPHook</executorPath>
        </hook>
        <hook>
            <displayName>RemoteServiceInvocation [RSI]</displayName>
            <priorityID>3</priorityID>
            <priority>RUNNER</priority>
            <executorPath>me.moonways.bridgenet.bootstrap.hook.rsi.ServiceInitHook</executorPath>
        </hook>
        <hook>
            <displayName>ConsoleStart</displayName>
            <priority>POST_RUNNER</priority>
            <executorPath>me.moonways.bridgenet.bootstrap.hook.console.ApacheConsoleStartHook</executorPath>
        </hook>
        <hook>
            <displayName>Goodbye</displayName>
            <priority>SHUTDOWN</priority>
            <executorPath>me.moonways.bridgenet.bootstrap.hook.goodbye.GoodbyeHook</executorPath>
        </hook>
    </hooks>
</bootstrap>
